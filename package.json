    "jest-date-mock": "^1.0.8",
    "metro-react-native-babel-preset": "^0.66.2",
    "node-fetch": "2.6.7",
    "prettier": "^1.19.1",
    "promise": "~8.1.0",
    "react-native-cli": "^2.0.1",
