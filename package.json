    "react-native-splash-screen": "^3.3.0",
    "react-native-sse": "^1.1.0",
    "react-native-svg": "^12.3.0",
    "react-native-uncontrolled-date-picker-ios": "^1.0.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-video": "^5.2.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
